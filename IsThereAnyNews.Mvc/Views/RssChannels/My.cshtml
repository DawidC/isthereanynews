@model IsThereAnyNews.ViewModels.RssChannelsMyViewModel

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="col-lg-3">
        @foreach (var channel in Model.ChannelsSubscriptions)
        {
            @Html.ActionLink(channel.Title, "Index", "RssChannel", new { id = channel.Id }, new { @class = "btn btn-default btn-lg btn-block btn-xs" })
        }
    </div>
    <div class="col-lg-9"></div>
</div>
